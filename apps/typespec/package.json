{
    "name": "@mfskillsearch/typespec",
    "version": "1.0.0",
    "description": "TypeSpec API definitions for PFSkillSearch",
    "scripts": {
        "spec": "tsp compile api/main.tsp --config tspconfig.yaml",
        "postspec": "npm run openapi",
        "openapi": "npx openapi-typescript ./dist/@typespec/openapi3/openapi.yaml -o ./dist/typescript/index.d.ts -t --root-types --enum  --enum-values",
        "tsp-install": "tsp install",
        "build": "npm run spec"
    },
    "devDependencies": {
        "rimraf": "^6.0.1",
        "@typespec/compiler": "^1.2.1",
        "@typespec/http": "^1.2.1",
        "@typespec/rest": "^0.72.1",
        "@typespec/openapi3": "^1.2.1",
        "@typespec/json-schema": "^1.2.1"
    },
    "private": true
}